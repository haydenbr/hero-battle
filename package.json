{
	"name": "hero-battle",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"ng": "ng",
		"start": "yarn serve",
		"serve": "ng serve --host 0.0.0.0",
		"build": "ng build --source-map --output-path dist",
		"lint": "tslint -p . --fix",
		"bump": "./scripts/bump.js",
		"precommit": "lint-staged && yarn prettier",
